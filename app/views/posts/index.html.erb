<div class="posts-wrapper row">
<% @posts.each do |post| %>
    <div class="post">
      <div class="post-head">
        <div class="name">Angelman<div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag post.image.representation(resize: "640x")), post_path(post) %>
      </div>
      <p class="caption"><%= post.caption %></p>
    <div>
<% end %>
</div>